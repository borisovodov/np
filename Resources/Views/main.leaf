                    <input type="text" class="form-control" name="query" placeholder="Search" >
                    <div class="input-group-append">
                        <button class="btn btn-outline-secondary" type="submit"><img width="16" height="16" src="/searchIcon.png"></button>
                    </div>
            </div>

<div class="col-md-3 col-sm-6 col-12 px-2 mb-3 d-flex">
    <div class="card bg-light flex-grow-1">
        <img class="card-img-top mx-auto d-block square" src="">
        <div class="card-img-overlay">
            <h4 class="card-title"><a class="card-title-link" href="#(city.URL)">#(city.name)</a></h4>
            <p class="card-text">#(city.newspapersCount) #if(city.newspapersCount != 1):newspapers#else:newspaper#endif</p>
        </div>
    </div>
</div>
